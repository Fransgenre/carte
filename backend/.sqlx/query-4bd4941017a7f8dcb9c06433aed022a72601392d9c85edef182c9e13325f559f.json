  "query": "\n            UPDATE categories\n            SET title = $2, family_id = $3, default_status = $4, fill_color = $5, border_color = $6, version = $7\n            WHERE id = $1\n            RETURNING\n                id,\n                title,\n                family_id,\n                default_status,\n                (SELECT hash FROM icons WHERE id = icon_id) AS icon_hash,\n                fill_color,\n                border_color,\n                version,\n                created_at,\n                updated_at\n            ",
